{
  "name": "titledk/silverstripe-starter",
  "description": "Gulp & npm based frontend development setup for SilverStripe and other projects, leveraging live reloading & Babel for ES6 and other cool stuff",
  "require": {
    "php": ">=5.6",
    "silverstripe/cms": "3.4.x-dev",
    "silverstripe/framework": "3.4.x-dev"
  },
  "require-dev": {
    "phpunit/PHPUnit": "~3.7",
    "froog/silvergraph": "dev-master",
    "axyr/silverstripe-ideannotator": "dev-master",
    "gdmedia/ss-auto-git-ignore": "dev-master"
  },
  "config": {
    "process-timeout": 600
  },
  "scripts": {
    "post-update-cmd": "GDM\\SSAutoGitIgnore\\UpdateScript::Go"
  },
  "prefer-stable": true,
  "minimum-stability": "dev"
}
